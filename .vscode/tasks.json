{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc-9 生成活动文件",
			"command": "/usr/bin/gcc-9",
			"args": [
				"${fileDirname}/mynftw.o", // ${fileDirname}指的是要debug的文件所在的目录地址
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}",
				"-Wall",
				"-ltlpi",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "调试器生成的任务。"
		}
	],
	"version": "2.0.0"
}