	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc-9",
			"command": "/usr/bin/gcc-9",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/echo_sv.out",
				"-Wall",
				"-ltlpi"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "调试器生成的任务。"
		}
